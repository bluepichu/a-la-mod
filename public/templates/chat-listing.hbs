<ala-chat-listing chat-id="{{id}}">
    <ala-color-stripe style="background: #2196F3"></ala-color-stripe>
    <ala-chat-title>{{title}}</ala-chat-title>
    <ala-members-list>{{members}}</ala-members-list>
    <ala-most-recent>{{#if mostRecent}}{{mostRecent.sender}}: {{mostRecent.message}}{{else}}No messages yet.{{/if}}</ala-most-recent>
    <ala-unread-count>0</ala-unread-count>
</ala-chat-listing>